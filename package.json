{
  "name": "startec-crawler",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/StartecJobsDev/startec-crawler.git",
  "author": "Startec <produto@startecjobs.com>",
  "license": "MIT",
  "scripts": {
    "start:api": "node dist/infra/server/index.js",
    "start:queue": "node dist/infra/queue-provider/index.js",
    "dev:api": "tsnd --respawn --transpile-only --ignore-watch  node_modules --exit-child source/infra/server/index.ts",
    "dev:queue": "tsnd --respawn --transpile-only --ignore-watch  node_modules --exit-child source/infra/queue-provider/index.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watchAll",
    "test:staged": "npm test -- --findRelatedTests",
    "prisma:init": "yarn prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "5.5.9",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.7",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@vercel/node": "1.12.1",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "5.5.3",
    "git-commit-msg-linter": "3.2.8",
    "husky": "^7.0.2",
    "jest": "^27.4.7",
    "lint-staged": "^11.1.2",
    "prettier": "^2.0.5",
    "prisma": "^4.1.0",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "tsnd": "^1.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@bull-board/express": "^3.11.1",
    "@prisma/client": "^4.1.0",
    "@sendgrid/mail": "^7.6.2",
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "@types/bull": "^3.15.8",
    "bcryptjs": "^2.4.3",
    "bull": "^4.8.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "glob": "^8.0.3",
    "ioredis": "^5.0.5",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^3.3.1",
    "openai": "^3.2.1",
    "querystring": "^0.2.1",
    "uuid": "^8.3.2",
    "youch": "^3.2.0"
  }
}
